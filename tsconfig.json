{
  "compilerOptions": {
    "target": "ES2020",
  "module": "NodeNext",
    "jsx": "react-native",
    "allowJs": true,
    "strict": true,
    "noEmit": true,
  "moduleResolution": "nodenext",
    "types": [
      "react",
      "react-native",
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": [
    "**/*"
  ],
  "extends": "expo/tsconfig.base"
}
